{
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "dotenv.enableAutocloaking": false,
  "cSpell.words": [
    "chainlog",
    "delv",
    "delvtech",
    "dnum",
    "fixedpointmath",
    "hyperdrive",
    "hyperwasm",
    "ihyperdrive",
    "mintable",
    "numberish",
    "pageview",
    "pageviews",
    "reimagined",
    "signale",
    "tanstack",
    "thiserror",
    "viem"
  ],
  "typescript.tsdk": "node_modules/typescript/lib",

  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
