{
  "editor.codeActionsOnSave": {
    "source.organizeImports.biome": "explicit",
    "source.removeUnusedImports": "explicit"
  },
  "dotenv.enableAutocloaking": false,
  "cSpell.words": [
    "chainlog",
    "delv",
    "delvtech",
    "dnum",
    "fixedpointmath",
    "hyperdrive",
    "hyperwasm",
    "ihyperdrive",
    "mintable",
    "numberish",
    "pageview",
    "pageviews",
    "reimagined",
    "signale",
    "tanstack",
    "thiserror",
    "viem"
  ],
  "prettier.configPath": ".prettierrc.mjs",
  "typescript.tsdk": "node_modules/typescript/lib",
  "biome.lspBin": "./node_modules/.bin/biome",
  "[javascript]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[typescript]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[json]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "biomejs.biome"
  }
}
