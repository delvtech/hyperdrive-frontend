{
  "name": "@delvtech/hyperdrive-appconfig",
  "version": "0.0.1",
  "license": "AGPL-3.0",
  "type": "module",
  "main": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "export": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": {
        "import": "./dist/esm/index.d.ts",
        "require": "./dist/cjs/index.d.ts"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "generate": "npx tsx ./src/scripts/generate.ts",
    "build:esm": "tsc",
    "build:commonjs": "tsc --project tsconfig.cjs.json", 
    "build2":"yarn run build:esm && yarn run build:commonjs",
    "build": "tsup src/index.ts --minify --format esm,cjs --clean && tsc --emitDeclarationOnly --outDir dist/esm",
    "watch": "tsup src/index.ts --watch --minify --format esm,cjs --dts",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@delvtech/fixed-point-wasm": "^0.0.6",
    "@delvtech/hyperdrive-artifacts": "^1.0.18",
    "@delvtech/hyperdrive-viem": "^3.0.6",
    "@hyperdrive/eslint-config": "*",
    "@hyperdrive/prettier-config": "*",
    "@hyperdrive/tsconfig": "*",
    "@types/lodash.camelcase": "^4.3.9",
    "@types/lodash.uniqby": "^4.7.9",
    "abitype": "^0.9.8",
    "chalk": "^5.3.0",
    "dotenv": "^16.0.3",
    "lodash.camelcase": "^4.3.0",
    "lodash.uniqby": "^4.7.0",
    "prettier-plugin-organize-imports": "3.2.4",
    "tsconfig-paths": "^4.1.0",
    "tsup": "^7.2.0",
    "tsx": "^4.7.0",
    "typescript": "^5.0.2",
    "vite-tsconfig-paths": "^4.2.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "graphql-request": "^7.1.0",
    "viem": "^2.7.8"
  }
}
