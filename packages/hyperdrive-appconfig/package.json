    "@hyperdrive/tsconfig": "*",
    "abitype": "^0.9.8",
    "dotenv": "^16.0.3",
    "tsx": "^4.7.0",
    "prettier-plugin-organize-imports": "^3.2.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "tsup": "^7.2.0",
    "typescript": "^5.0.2",
    "vite-tsconfig-paths": "^4.2.0"
  },
  "publishConfig": {
    "access": "public"
