{
  "name": "@delvtech/hyperdrive-js",
  "version": "0.0.1",
  "license": "AGPL-3.0",
  "type": "module",
  "scripts": {
    "build": "tsup",
    "watch": "npm run build -- --watch",
    "test": "vitest run",
    "test:watch": "vitest --reporter=verbose",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@delvtech/drift": "^0.1.1",
    "@delvtech/fixed-point-wasm": "^0.0.7",
    "@delvtech/hyperdrive-artifacts": "^1.0.20",
    "@delvtech/hyperdrive-wasm": "^0.16.0",
    "lodash.groupby": "^4.6.0",
    "lodash.mapvalues": "^4.6.0",
    "semver": "^7.7.1"
  },
  "devDependencies": {
    "@hyperdrive/eslint-config": "*",
    "@hyperdrive/prettier-config": "*",
    "@hyperdrive/tsconfig": "*",
    "@types/lodash.groupby": "^4.6.9",
    "@types/lodash.mapvalues": "^4.6.9",
    "@types/sinon": "^17.0.3",
    "abitype": "^1.0.8",
    "dotenv": "^16.4.7",
    "prettier": "3.4.2",
    "prettier-plugin-organize-imports": "4.1.0",
    "sinon": "^18.0.0",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.5"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./v1.0.14": {
      "types": "./dist/v1.0.14.d.ts",
      "require": "./dist/v1.0.14.cjs",
      "default": "./dist/v1.0.14.js"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  }
}
