{
  "name": "@hyperdrive/artifacts",
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "files": [
    "artifacts"
  ],
  "exports": {
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js"
    },
    "./*.js": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build:src": "sh scripts/build_src.sh src",
    "build": "npm run build:src && tsc --build --force"
  }
}
