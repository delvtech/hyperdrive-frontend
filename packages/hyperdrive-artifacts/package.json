{
  "name": "@hyperdrive/artifacts",
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js"
    },
    "./*.js": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build:dist": "npm run build:src && tsc --build --force",
    "build:src": "sh scripts/build_src.sh v0.7.0",
    "build:new": "rm -rf src && npm run build:src "
  },
  "devDependencies": {
    "@hyperdrive/tsconfig": "*",
    "tsc": "^2.0.4",
    "typescript": "^5.3.3"
  }
}
