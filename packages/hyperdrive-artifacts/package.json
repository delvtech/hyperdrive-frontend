{
  "name": "@delvtech/hyperdrive-artifacts",
  "version": "1.0.21",
  "description": "Strongly typed build artifacts, including ABIs and Bytecodes, for the Hyperdrive AMM.",
  "license": "Apache-2.0",
  "author": "DELV (https://github.com/delvtech)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/delvtech/hyperdrive-frontend.git",
    "directory": "packages/hyperdrive-artifacts"
  },
  "type": "module",
  "sideEffects": false,
  "exports": {
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js"
    },
    "./*.js": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "generate": "sh scripts/generate.sh v1.0.20",
    "build": "tsc --build"
  },
  "devDependencies": {
    "@hyperdrive/tsconfig": "*",
    "tsc": "^2.0.4",
    "typescript": "^5.9.2"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  }
}
